{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "vercel-build": "node scripts/migrate.js && next build --turbopack",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "typecheck": "tsc --noEmit",
    "prepare": "husky",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@sentry/nextjs": "^10.38.0",
    "drizzle-orm": "^0.38.3",
    "linkedom": "^0.18.12",
    "next": "15.5.10",
    "postgres": "^3.4.5",
    "posthog-js": "^1.335.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-force-graph-2d": "^1.29.0",
    "resend": "^6.8.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "drizzle-kit": "^0.30.1",
    "eslint": "^9.36.0",
    "eslint-config-next": "^15.5.7",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.1",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "typescript": "5.9.3"
  }
}
